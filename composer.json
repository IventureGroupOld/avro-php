  "require": {
    "php": "^5.6||^7"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^0.9",
    "phpunit/phpunit": "^5"
  },
  "suggest": {
    "ext-gmp": "Large integer support for 32-bit platforms."
  },
  "autoload": {
    "classmap": ["lib/"]
  },
  "scripts": {
    "test": [
      "parallel-lint . --exclude vendor",
      "phpunit -c phpunit.xml"
    ]
  }
